<!doctype html>
<html>
<head>
  <title>Lisa Stephen Photography</title>
  <link href="https://fonts.googleapis.com/css?family=Shadows+Into+Light" rel="stylesheet">
  <link href="https://fonts.googleapis.com/css?family=Raleway" rel="stylesheet">
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
  <link type="text/css" rel="stylesheet" href="css/lightgallery.css" /> 
  <link rel="stylesheet" type="text/css" href="css/index.css">
</head>
<body class="home">
  <div class="wrapper">
    <div class="header-container">
      <%- include('partials/navbar') %>
    </div>
    <div class="image-container">
      <div class="image" id="image1">
        <a href="#" id="launch">
          <img src="/img/EW-gallery/20160209 Emily Wold-1.jpg"></img>
        </a>
      </div>
      <div class="image" id="image2">
        <a href="#">
          <img src="/img/UZ-gallery/20170629 Ulyana Zilbermints-55.jpg"></img>
        </a>
      </div>
      <div class="image" id="image3">
        <a href="#">
          <img src="/img/EW-gallery/20160209 Emily Wold-4.jpg"></img>
        </a>
      </div>
    </div>
  </div>
  <script src="js/jquery.min.js"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script>

  <!-- A jQuery plugin that adds cross-browser mouse wheel support. (Optional) -->
  <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-mousewheel/3.1.13/jquery.mousewheel.min.js"></script>

  <script src="js/lightgallery.min.js"></script>

  <!-- lightgallery plugins -->
  <script src="js/lg-thumbnail.min.js"></script>
  <script src="js/lg-fullscreen.min.js"></script>
  <script>
    $('#image1, #image3, #gallery').on('click', function() {
   
      var arr = [];

      var pos = [1,3,4,8,10,26,27,31,34,36,39,40,42,43,44,45,46,47,49,54,57,110,111,116,126,141,144,155,160,163,164,168,175,178,180,182,187,192,193,198,203,215,216,221,225,229,235,237,238,240,242,246,247,248,262];

      for (var x = 0; x <= pos.length; x++) {
        arr.push({
            "src": '/img/EW-gallery/20160209 Emily Wold-'+pos[x]+'.jpg',
            'thumb': '/img/EW-thumbnails/20160209 Emily Wold-'+pos[x]+'.jpg',
            'subHtml': ''
        })
      }
      $(this).lightGallery({
          dynamic: true,
          download: false,
          dynamicEl: arr
      })
   
    });

    $('#image2').on('click', function() {
   
      var arr = [];

      var pos = [1,2,3,6,9,11,16,18,19,21,22,23,25,27,31,32,34,35,36,38,39,42,44,46,47,50,51,52,53,54,55,58,59,60,62,64,65,70,73,82,83,85,86,88,89,93,94,100,101,105,106,107,108];

      for (var x = 0; x <= pos.length; x++) {
        arr.push({
            "src": '/img/UZ-gallery/20170629 Ulyana Zilbermints-'+pos[x]+'.jpg',
            'thumb': '/img/UZ-thumbnails/20170629 Ulyana Zilbermints-'+pos[x]+'.jpg',
            'subHtml': ''
        })
      }
      $(this).lightGallery({
          dynamic: true,
          download: false,
          dynamicEl: arr
      })
   
    });

    document.addEventListener('contextmenu', event => event.preventDefault());
  </script>
</body>
</html>
